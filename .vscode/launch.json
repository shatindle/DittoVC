    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\index.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Check Servers",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server-check.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Check Localizations",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\testLocalizations.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Test Channel Cleanup",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/testCleanup.js"
        }
    ]
}